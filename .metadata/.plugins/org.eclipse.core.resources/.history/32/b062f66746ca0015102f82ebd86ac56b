	colid : '',
	text : '',
	menuDisabled : true,
	sortable : false,
	maxWidth : 30,
	minWidth : 30,
	width : 30,
	// imgUrl : './../../images/button_icon/ToolBarSearch.png',
	renderer : function(value) {
